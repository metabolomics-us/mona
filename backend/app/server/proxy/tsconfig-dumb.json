{
  "compilerOptions": {
    "baseUrl": "",
    "declaration": false,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "lib": ["es2015", "dom"],
    "mapRoot": "./",
    "module": "commonjs",
    "moduleResolution": "Node",
    "outDir": "../dist/out-tsc",
    "skipLibCheck": true,
    "sourceMap": true,
    "target": "es5",
    "types": ["node"],
    "typeRoots": [
      "./node_modules/@types"
    ]
  },
  "exclude": ["./src/main/app/lib/chemdoodle/ChemDoodleWeb.css", "./node_modules/**",
              "./node_modules/angular-dialog-service/dist/dialogs.css"]
}
