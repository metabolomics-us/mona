{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "id": "http://mona.fiehnlab.ucdavis.edu/schema/similarityResult",
  "type": "object",
  "properties": {
    "result": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "id": {
            "type": "integer"
          },
          "similarity": {
            "type": "number"
          }
        }
      }
    },
    "statistics": {
      "type": "object",
      "properties": {
        "duration": {
          "type": "integer"
        }
      }
    },
    "config": {
      "type": "object",
      "properties": {
        "minSimilarity": {
          "type": "number"
        },
        "commonIonCount": {
          "type": "integer"
        },
        "spectra": {
          "type": "string"
        },
        "maxHits": {
          "type": "integer"
        }
      }
    }
  }
}